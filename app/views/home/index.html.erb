<h1>Adnat</h1>
<% if logged_in? %>
  <h3>Logged in as <%=current_user.name%></h3>
<% end %>
<% if !logged_in? %>
  <%=button_to "Sign Up", signup_path, :method => :get, class:"btn btn-outline-secondary"%>
  <%=button_to "Log In", login_path, :method => :get, class:"btn btn-outline-secondary"%>
<% end %>